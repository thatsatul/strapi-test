{
  "kind": "collectionType",
  "collectionName": "offers",
  "info": {
    "name": "Offers",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid"
    },
    "logo_url": {
      "type": "string"
    },
    "is_active": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "is_featured": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "product_type": {
      "type": "enumeration",
      "enum": [
        "postpaid_plus",
        "wallet",
        "credit_card"
      ]
    },
    "title": {
      "type": "string"
    },
    "offer_cta_text": {
      "type": "string"
    },
    "offer_cta_url": {
      "type": "string"
    }
  }
}
